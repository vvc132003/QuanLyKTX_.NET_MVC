@model List<QuanLyKyTucXa_MVC.Models.Modeldata>
<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet"
      integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
<link rel='stylesheet'
      href='https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css'>
<link href="/static/morris.css" rel="stylesheet" />
<link href="/static/c3.min.css" rel="stylesheet" />
<link href="/static/home.css" rel="stylesheet" />
<link href="/static/dashboard1.css" rel="stylesheet" />
<link href="/static/style.css" rel="stylesheet" />
<div class="skin-default-dark fixed-layout">
    <div id="main-wrapper">
        @Html.Partial("~/Views/layout/navbar.cshtml")
        @Html.Partial("~/Views/layout/header.cshtml")
        <br />
        <div class="page-wrapper">
            <div class="container-fluid">
                <div class="row">
                    <div class="col-12">
                        <div class="card">
                            <div class="card-body">
                                <div class="table-responsive">
                                    <table class="table">
                                        <thead>
                                            <tr>
                                                <th>ID</th>
                                                <th>Tên sinh viên</th>
                                                <th>Email</th>
                                                <th>Sđt</th>
                                                <th>Gioi tính</th>
                                                <th>Số lần vi phạm</th>
                                                <th>Số phòng</th>
                                                <th>Ngày sinh</th>
                                                <th>Ngày vào</th>
                                                <th>Trạng thái</th>
                                                <th>Action</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            @if (Model != null && Model.Count > 0)
                                            {
                                                foreach (var sinhvien in Model)
                                                {
                                                    <tr>
                                                        <td>@sinhvien.sinhVien.id</td>
                                                        <td>@sinhvien.sinhVien.tensinhvien</td>
                                                        <td>@sinhvien.sinhVien.email</td>
                                                        <td>@sinhvien.sinhVien.sodienthoai</td>
                                                        <td>@sinhvien.sinhVien.gioitinh</td>
                                                        <td>@sinhvien.sinhVien.solanvipham</td>
                                                        <td>@sinhvien.sinhVien.idphong</td>
                                                        <td>@sinhvien.sinhVien.ngaysinh</td>
                                                        <td>@sinhvien.sinhVien.ngayvao</td>
                                                        <td>@sinhvien.sinhVien.trang_thai</td>
                                                        <td>
                                                            <a href="/SinhVien/SinhVienChuyenPhong/@sinhvien.sinhVien.id" class="btn btn-primary" title="Chuyển phòng">
                                                                <i class="fas fa-exchange-alt"></i>
                                                            </a>
                                                            <a href="/SinhVien/SinhVienTraPhong/@sinhvien.sinhVien.id" class="btn btn-primary" title="Trả phòng">
                                                                <i class="fas fa-sign-out-alt"></i>
                                                            </a>
                                                        </td>
                                                    </tr>
                                                }
                                            }
                                            else
                                            {
                                                <tr>
                                                    <td colspan="5">Không có sinh viên nào trong phòng.</td>
                                                </tr>
                                            }
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/js/bootstrap.min.js"></script>
<script src="https://cdn.jsdelivr.net/npm/popper.js@2.9.3/dist/umd/popper.min.js"></script>
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/js/bootstrap.bundle.min.js"
        integrity="sha384-geWF76RCwLtnZ8qwWowPQNguL3RmwHVBC9FhGdlKrxdiJJigb/j/68SIy3Te4Bkz"
        crossorigin="anonymous"></script>
<script src="/static/custom.min.js"></script>
<script src="/static/jquery-3.2.1.min.js"></script>