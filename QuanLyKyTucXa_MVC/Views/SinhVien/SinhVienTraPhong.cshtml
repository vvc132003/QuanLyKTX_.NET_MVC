@model QuanLyKyTucXa_MVC.Models.Modeldata

<form action="/SinhVien/TraPhong" method="post">
    <div class="form-group">
        <input type="hidden" id="id" name="id" value="@Model.sinhVien.id" class="form-control" />
        
    </div>
    <div class="form-group">
        <label for="lydo">lydo</label>
        <input type="text" id="lydo" name="lydo"  class="form-control" />
    </div>
    <div class="form-group">
        <label for="ngaytra">ngaytra</label>
        <input type="date" id="ngaytra" name="ngaytra"  class="form-control" />
    </div>
    <button type="submit" class="btn btn-primary">Update</button>
</form>
