@model QuanLyKyTucXa_MVC.Models.SinhVien

<form action="/Product/Edit" method="post">
    <div class="form-group">
        <input type="hidden" id="Id" name="Id" value="@Model.id" class="form-control" />
        <label for="tensinhvien">Tên sinh viên</label>
        <input type="text" id="tensinhvien" name="tensinhvien" value="@Model.tensinhvien" class="form-control" />
    </div>
    <div class="form-group">
        <label for="idphong">Id phòng</label>
        <input type="text" id="idphong" name="idphong" value="@Model.idphong" class="form-control" />
    </div>
    <button type="submit" class="btn btn-primary">Update</button>
</form>
